<section id="section_settings_q_matic" style="display: none;">
    <p>{% translate language, 'common', 'q-matic-info' %}</p>
    <form class="form-horizontal">
        <div class="control-group">
            <label class="control-label" for="qmatic_url">{% translate language, 'common', 'Url' %}</label>
            <div class="controls">
                <input type="url" id="qmatic_url" autocomplete="off" style="width: 300px;">
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="qmatic_auth_token">{% translate language, 'common', 'Secret' %}</label>
            <div class="controls">
                <input type="text" id="qmatic_auth_token" autocomplete="off" style="width: 300px;">
            </div>
        </div>
        <div class="control-group">
            <div class="controls">
                <button id="btn_save_qmatic_settings" type="button" class="btn btn-success">
                  {% translate language, 'common' ,'Save' %}
                </button>
            </div>
        </div>
    </form>
</section>
