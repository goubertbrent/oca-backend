{
  "name": "oca-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "python fix-agm-maps.py",
    "start": "ng serve --configuration hmr",
    "start-web": "ng serve --project web",
    "start-console": "ng serve --project console",
    "build": "npm run build-libraries && npm run build-dashboard && npm run build-web && npm run build-console",
    "build-libraries": "ng build --project web-shared",
    "build-dashboard": "ng build --prod --project oca-dashboard --base-href=/static/client",
    "build-web": "ng build --prod --project web && cp ../src/static/web/index.html ../src/rogerthat/templates/web-app.html",
    "build-console": "ng build --prod --project console --base-href=/console && cp ../src/static/console/index.html ../src/shop/html/console-index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^9.1.4",
    "@angular/cdk": "^9.2.1",
